{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "lib": [
      "dom",
      "dom.iterable",
      "ES6"
    ],
    "jsx": "react",
    "outDir": "build",
    "sourceMap": true,
    "declaration": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "src"
, "../ui", "../ui/preview/vite.config.ts"  ],
  "exclude": [
    "node_modules",
    "src/setupTests.js",
    "src/**/__tests__",
    "**/*.test.*",
    "**/*.spec.*"
  ]
}
