{
  "name": "kemis-library",
  "version": "2.3.65",
  "description": "Kemis package for creating forms, which mainly uses the 'primereact' library.",
  "homepage": "https://github.com/KEMIS-Systems/components",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "components/**",
    "hooks/**",
    "models/**",
    "utils/**",
    "components.js",
    "utils.js",
    ".babelrc",
    "index.js",
    "index.js.map",
    "index.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "postbuild": "node ./copyFiles.js",
    "test": "jest --watchAll --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src",
    "version": "git add package.json && git commit -m 'Bump version'",
    "merge": "git pull && git push origin v2:master && git checkout master && git pull && git checkout v2",
    "auto_publish": "yarn merge && rm -Rf build && yarn build && cd build && yarn publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KEMIS-Systems/components.git"
  },
  "keywords": [
    "react",
